{
  "name": "e-com",
  "version": "1.0.0",
  "description": "e-commerce",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "dev": "nodemon index.js",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js --disable-host-check",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/keniossai/e-commerce-store.git"
  },
  "author": "kenneth",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keniossai/e-commerce-store/issues"
  },
  "homepage": "https://github.com/keniossai/e-commerce-store#readme",
  "dependencies": {
    "express": "^4.17.1",
    "express-edge": "^2.0.2",
    "express-fileupload": "^1.2.1",
    "mongoose": "^5.11.19"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "laravel-mix": "^6.0.13",
    "nodemon": "^2.0.7",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1"
  }
}
